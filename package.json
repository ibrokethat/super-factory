{
  "name": "@ibrokethat/factory",
  "description": "factories the functional way",
  "version": "1.0.1",
  "author": "si@ibrokethat.com",
  "contributors": [
    { "name": "Simon Jefford", "email": "si@ibrokethat.com" }
  ],
  "dependencies": {
    "@ibrokethat/curry": "1.0.x",
    "@ibrokethat/iter": "1.0.x",
    "babel": "5.0.x"
  },
  "devDependencies": {
    "chai": "latest",
    "mocha": "latest",
    "sinon": "latest"
  },
  "keywords": [
    "ibrokethat",
    "pipeline",
    "es6",
    "functional factory"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ibrokethat/super-factory"
  },
  "main": "lib/index.js",
  "scripts": {
    "build":"./node_modules/.bin/babel ./*.js --out-dir lib --source-maps" ,
    "build-watch":"./node_modules/.bin/babel ./*.js --out-dir lib --source-maps -w" ,
    "prepublish" : "npm prune",
    "postinstall" : "npm run build",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register --harmony --colors --recursive -R spec --ui bdd ./test/**/*.test.js --NODE_ENV=test"
  },
  "engines": {
    "node": "0.12.x"
  }
}
